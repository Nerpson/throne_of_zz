<div id="houses">
    <h2> La bonne liste des maisons</h2>
    <ul>
        <!-- generated by jquery-->
    </ul>
</div>

<script type="text/javascript">
    // jquery
    $(document).ready(function () {

    var api = new Api("http://localhost:3716/api/");
    var houses = new HouseAPI(api);
    
    var liste = 
    houses.getAllHouses()
        .then(function (hs) {
            var container = $('#houses>ul');
            hs.forEach(function (h) {
                container.append('<li>' + h.ID + '|' + h.Name + '</li>');
            });
         });
    });
</script>
